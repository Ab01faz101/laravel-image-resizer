{
    "name": "ab01faz101/laravel-image-resizer",
    "description": "Resize image to multiple sizes in Laravel",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Ab01faz101\\LaravelImageResizer\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Abolfazl Qaederahmat",
            "email": "ar.rahmatyy@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "laravel/framework": "^11.0 || ^12.0",
        "intervention/image-laravel": "^1.5"
    },
    "keywords": [
        "livewire",
        "image",
        "image resize",
        "image size",
        "laravel image resize",
        "laravel"
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Ab01faz101\\LaravelImageResizer\\LaravelImageResizerServiceProvider"
            ]
        }
    }
}
